{
  "name": "react-app-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:fresh": "npm run build && npm start",
    "build": "npm run styles:build && npm run js:build",
    "js": "./node_modules/.bin/webpack",
    "js:watch": "npm run js -- -w",
    "js:build": "npm run js -- -p",
    "styles": "./node_modules/.bin/node-sass --importer node_modules/node-sass-glob-importer/dist/cli.js ./src/styles/app.scss ./dist/app.css",
    "styles:build": "npm run styles",
    "styles:watch": "npm run styles -- -wr"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babelify": "^7.3.0",
    "eslint": "^3.14.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "faker": "^3.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "node-sass": "^4.3.0",
    "node-sass-glob-importer": "^3.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-gateway": "^2.6.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
